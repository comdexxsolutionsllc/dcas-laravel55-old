{
  "name": "fiberhopllc_dcas",
  "description": "FiberHop LLC - DCAS - Datacenter Automation Suite",
  "keywords": [
    "framework",
    "laravel",
    "dcas",
    "web hosting",
    "datacenter",
    "fiberhop"
  ],
  "license": "Closed Source",
  "type": "project",
  "require": {
    "php": ">=7.0.0",
    "anlutro/l4-settings": "^0.5.0",
    "artem-schander/l5-modular": "^1.4",
    "barryvdh/laravel-cors": "^0.9.3",
    "barryvdh/laravel-ide-helper": "^2.4",
    "doctrine/dbal": "^2.6",
    "dompdf/dompdf": "v0.7.0",
    "fideloper/proxy": "~3.3",
    "geoip/geoip": "^1.17",
    "jeroennoten/laravel-adminlte": "^1.20",
    "jlapp/swaggervel": "dev-master",
    "kalnoy/nestedset": "^4.2",
    "laracasts/presenter": "^0.2.1",
    "laravel/cashier": "~7.0",
    "laravel/envoy": "~1.0",
    "laravel/framework": "5.5.*",
    "laravel/passport": "*",
    "laravel/scout": "*",
    "laravel/socialite": "*",
    "laravel/tinker": "~1.0",
    "laravelcollective/html": "*",
    "league/pipeline": "^0.3.0",
    "league/uri": "^5.0",
    "mercuryseries/flashy": "^1.0",
    "predis/predis": "^1.1",
    "pusher/pusher-php-server": "^2.6",
    "spatie/array-to-xml": "^2.5",
    "spatie/laravel-fractal": "^5.0",
    "stripe/stripe-php": "~3.0",
    "symfony/css-selector": "3.1.*",
    "symfony/dom-crawler": "3.1.*",
    "tucker-eric/eloquentfilter": "^1.2",
    "tylercd100/lern": "^4.1",
    "venturecraft/revisionable": "^1.28",
    "watson/rememberable": "^2.0",
    "yab/laravel-scout-mysql-driver": "dev-master",
    "yajra/laravel-datatables-oracle": "~8.0",
    "zizaco/entrust": "5.2.x-dev"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.0",
    "behat/behat": "^3.3",
    "behat/mink": "^1.7",
    "behat/mink-browserkit-driver": "^1.3",
    "behat/mink-extension": "^2.2",
    "codeception/codeception": "*",
    "filp/whoops": "~2.0",
    "fzaninotto/faker": "~1.4",
    "laracasts/behat-laravel-extension": "^1.0",
    "laracasts/generators": "dev-master as 1.1.4",
    "laravel/dusk": "^2.0",
    "maddhatter/laravel-view-generator": "^0.0.4",
    "mockery/mockery": "0.9.*",
    "phpspec/phpspec": "^3.1",
    "phpunit/phpunit": "~6.0",
    "xethron/migrations-generator": "^2.0"
  },
  "autoload": {
    "classmap": [
      "database/seeds",
      "database/factories"
    ],
    "psr-4": {
      "App\\": "app/",
      "DCAS\\": "app/DCAS",
      "Modules\\": "app/Modules"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": [
      ]
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ],
    "post-update-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postUpdate",
      "@php artisan optimize",
      "@php artisan ide-helper:generate",
      "@php artisan ide-helper:meta",
      "@php artisan view:clear",
      "@php artisan cache:clear",
      "@composer dump-autoload"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
