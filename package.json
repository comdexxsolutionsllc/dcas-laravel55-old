    "ava": {
        "require": [
            "babel-register",
            "./tests/Javascript/helpers/setup-browser-env.js"
        ],
        "files": [
            "./tests/Javascript/**/*.js",
            "!./tests/Javascript/helpers/*.js"
        ],
        "concurrency": 5,
        "failFast": true,
        "tap": false,
        "powerAssert": false,
        "verbose": true
    },
    "babel": {
        "presets": [
            "es2015"
        ]
    }
