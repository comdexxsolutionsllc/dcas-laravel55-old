        "bourbon-neat": "^1.9.0",
        "bulma": "^0.2.3",
        "commander": "^2.11.0",
        "forever": "^0.15.3",
        "jquery": "^3.1.1",
        "laravel-echo": "^1.3.2",
        "lodash": "^4.17.4",
        "materialize": "^1.0.0",
        "materialize-css": "^0.98.2",
        "moment": "^2.17.1",
        "node-sass": "^4.5.3",
        "normalize.css": "^5.0.0",
        "pusher-js": "^4.1.0",
